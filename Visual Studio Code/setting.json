{
    "window.menuBarVisibility": "default",
    "window.zoomLevel": 0,
    "C_Cpp.updateChannel": "Insiders",
    "editor.renderWhitespace": "none",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "editor.renderControlCharacters": true,
    "code-runner.runInTerminal": true,
    "explorer.confirmDelete": false,
    "vsicons.dontShowNewVersionMessage": true,
    "explorer.confirmDragAndDrop": false,
    "files.autoGuessEncoding": true,
    "terminal.integrated.shell.linux": "/bin/bash",
    "terminal.integrated.shell.windows": "C:\\WINDOWS\\System32\\cmd.exe",
    "files.autoSave": "onFocusChange",
    "workbench.iconTheme": "vscode-icons",
    "verilog.linting.linter": "xvlog",
    "git.autofetch": true,

    // 默认打开使用的PDF阅读器
    "latex-workshop.view.pdf.viewer": "tab",
    // 外部PDF阅读器
    "latex-workshop.view.pdf.external.viewer.args": ["%PDF%"],
    "latex-workshop.view.pdf.external.viewer.command": "C:/Program Files (x86)/Adobe/Acrobat DC/Acrobat/Acrobat.exe",
    // 文件改变时是否build
    "latex-workshop.latex.autoBuild.run": "never",
    //
    "latex-workshop.view.pdf.external.synctex.args": [
        // 正向搜索
        "-forward-search",
        "%TEX%",
        "%LINE%",
        // 复用窗口实例,控制从外部pdf阅读器对tex代码进行反向搜索时不打开新窗口
        "-reuse-instance",
        // 反向搜索
        "-inverse-search",
        //前一个是vscode的本地路径,后一个是本地一个特定js的路径,一般只要把[username]改为你系统内的个人文件夹名字即可.
        // "\"C:\\Users\\15961\\AppData\\Local\\Programs\\Microsoft VS Code\\Code.exe\" \"C:\\Users\\15961\\AppData\\Local\\Programs\\Microsoft VS Code\\resources\\app\\out\\cli.js\" -r -g \"%f:%l\"",
        "%PDF%"
    ],
    // 编译工具和命令
    "latex-workshop.latex.tools": [
        {
            "name": "xelatex",
            "command": "xelatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",
                "%DOCFILE%"
            ]
        },
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",
                "-outdir=%OUTDIR%",
                "%DOCFILE%"
            ],
            "env": {}
        },
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOCFILE%"
            ],
            "env": {}
        },
        {
            "name": "bibtex",
            "command": "bibtex",
            "args": [
                "%DOCFILE%"
            ],
            "env": {}
        },
        {
            "name": "lualatexmk",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-lualatex",
                "-outdir=%OUTDIR%",
                "%DOCFILE%"
            ],
            "env": {}
        },
        {
            "name": "latexmk_rconly",
            "command": "latexmk",
            "args": [
                "%DOCFILE%"
            ],
            "env": {}
        }
    ]
}
